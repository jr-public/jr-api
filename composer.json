{
    "name": "jotaerre/jr-api",
    "description": "Template API",
	"version": "0.1.0",
    "require": {
        "firebase/php-jwt": "^6.0",
        "doctrine/orm": "^3",
        "doctrine/dbal": "^4",
        "symfony/cache": "^7",
        "symfony/http-foundation": "^7",
        "symfony/routing": "^7",
        "symfony/validator": "^7",
        "php-di/php-di":  "^7",
        "php": ">=8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Bootstrap\\": "src/Bootstrap/",
            "App\\Controller\\": "src/Controller/",
            "App\\DTO\\": "src/DTO/",
            "App\\Entity\\": "src/Entity/",
            "App\\Repository\\": "src/Repository/",
            "App\\Service\\": "src/Service/"
        },
        "files": [
            "src/functions.php"
        ]
    }
}


